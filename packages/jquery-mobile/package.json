    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/wq.app/issues"
    },
    "homepage": "https://github.com/wq/wq.app/tree/main/packages/jquery-mobile",
    "dependencies": {
        "@wq/store": "^1.3.0",
        "jquery": "^1.12.4",
        "mustache": "^4.0.1"
    }
