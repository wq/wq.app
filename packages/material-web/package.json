{
    "name": "@wq/material-web",
    "version": "1.3.0",
    "description": "Web bindings for @wq/material",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "test": "cd ../../ && npm run jest packages/material-web",
        "build": "npm run rollup && npm run babel && npm run prettier",
        "rollup": "cd ../../ && npm run rollup -- -c packages/material-web/rollup.config.js",
        "babel": "cd ../../ && npm run babel -- packages/material-web/src --out-dir packages/material-web/",
        "prettier": "cd ../../ && npm run prettier -- --write packages/material-web/",
        "lint": "cd ../../ && npm run eslint packages/material-web/{,src/,src/*/,src/*/*/}*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wq/wq.app.git",
        "directory": "packages/material-web"
    },
    "keywords": [
        "wq",
        "react",
        "material",
        "material-ui"
    ],
    "author": "S. Andrew Sheppard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/wq.app/issues"
    },
    "homepage": "https://wq.io/@wq/material",
    "dependencies": {
        "@date-io/date-fns": "^1.3.13",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@material-ui/pickers": "^3.2.10",
        "@wq/react": "file:../react",
        "date-fns": "^2.15.0",
        "formik-material-ui": "^3.0.0",
        "formik-material-ui-lab": "0.0.5",
        "formik-material-ui-pickers": "0.0.11",
        "material-ui-dropzone": "https://github.com/Yuvaleros/material-ui-dropzone/archive/4d5043c.tar.gz"
    },
    "devDependencies": {
        "@wq/model": "^1.3.0",
        "@wq/store": "^1.3.0",
        "react": "^16.14.0",
        "react-dom": "^16.14.0"
    }
}
