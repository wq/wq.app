{
    "name": "@wq/material-native",
    "version": "1.3.0",
    "description": "React Native and Expo bindings for @wq/material",
    "type": "module",
    "main": "src/index.js",
    "scripts": {
        "test": "cd ../../ && NODE_ENV=test_native npm run jest packages/material-native -- --config packages/material-native/jest.config.js --moduleDirectories packages/material-native/node_modules/ node_modules/",
        "build": "npm run prettier",
        "prettier": "cd ../../ && npm run prettier -- --write packages/material-native/",
        "lint": "cd ../../ && npm run eslint packages/material-native/{,src/,src/*/,src/*/*/}*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wq/wq.app.git",
        "directory": "packages/material-native"
    },
    "keywords": [
        "wq",
        "react",
        "material",
        "material-ui"
    ],
    "author": "S. Andrew Sheppard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/wq.app/issues"
    },
    "homepage": "https://wq.io/@wq/material",
    "dependencies": {
        "@wq/react": "file:../react",
        "date-fns": "^2.15.0"
    },
    "peerDependencies": {
        "@react-native-async-storage/async-storage": "*",
        "@react-native-community/datetimepicker": "*",
        "@react-native-community/masked-view": "*",
        "@react-native-community/netinfo": "*",
        "@react-navigation/native": "*",
        "@react-navigation/stack": "*",
        "react-native-modal-datetime-picker": "*",
        "react-native-paper": "*",
        "react-native-picker-select": "*"
    },
    "devDependencies": {
        "@react-native-async-storage/async-storage": "^1.17.10",
        "@react-native-community/datetimepicker": "^3.0.0",
        "@react-native-community/masked-view": "^0.1.10",
        "@react-native-community/netinfo": "^5.9.6",
        "@react-navigation/native": "^5.7.3",
        "@react-navigation/stack": "^5.14.9",
        "@wq/model": "^1.3.0",
        "@wq/store": "^1.3.0",
        "react-native-modal-datetime-picker": "^8.9.3",
        "react-native-paper": "^4.0.1",
        "react-native-picker-select": "^8.0.0"
    }
}
