        "@react-navigation/stack": "^6.3.11",
        "@wq/model": "^2.0.0-alpha.0",
        "@wq/store": "^2.0.0-alpha.0",
        "react-native-modal-datetime-picker": "^14.0.1",
        "react-native-paper": "^5.1.3",
        "react-native-picker-select": "^8.0.4"
    }
