{
    "name": "@wq/map",
    "version": "2.0.0",
    "description": "GIS integration for @wq/app pages",
    "type": "module",
    "main": "index.js",
    "react-native": "src/index.js",
    "scripts": {
        "test": "cd ../../ && npm run jest packages/map/",
        "build": "npm run babel && npm run prettier",
        "babel": "cd ../../ && npm run babel -- packages/map/src --out-dir packages/map/",
        "prettier": "cd ../../ && npm run prettier -- --write packages/map/",
        "lint": "cd ../../ && npm run eslint packages/map/{,src/,src/*/,src/*/*/}*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wq/wq.app.git",
        "directory": "packages/map"
    },
    "keywords": [
        "wq",
        "map",
        "gis"
    ],
    "author": "S. Andrew Sheppard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/wq.app/issues"
    },
    "homepage": "https://wq.io/@wq/map",
    "dependencies": {
        "@wq/material": "^2.0.0",
        "@wq/react": "^2.0.0",
        "mustache": "^4.2.0"
    },
    "devDependencies": {
        "@wq/material-web": "^2.0.0",
        "@wq/model": "^2.0.0",
        "@wq/store": "^2.0.0"
    }
